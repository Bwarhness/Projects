@model SchoolTHang.Models.Announcement
@{
    ViewBag.Title = "CreateAnnoucement";
}

<h2>CreateAnnoucement</h2>
@using (Html.BeginForm())
{    @Html.HiddenFor(p => p.ID_Subject, new { Value = ViewBag.SubjectID })
    <div class="form-group">
        @Html.LabelFor(p => p.Name, new { @class=""})
        @Html.TextBoxFor(p => p.Name, new { @class = "form-control" })
    </div>
    <div class="form-group">
        @Html.LabelFor(p => p.Content, new { @class = "" })
        @Html.TextAreaFor(p => p.Content, new { @class = "form-control" })
    </div>
    <button type="submit" class="btn btn-default">Submit</button>

}
